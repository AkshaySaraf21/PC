<?xml version="1.0"?>
<plugin
  interface="MessageTransport"
  name="emailMessageTransport">
  <plugin-java
    javaclass="com.guidewire.pl.plugin.email.internal.EmailMessageTransport">
    <param
      name="smtpPort"
      value="25"/>
    <param
      name="smtpHost"
      value="mailhost"/>
    <param
      name="defaultSenderName"
      value="Example Name"/>
    <param
      name="defaultSenderAddress"
      value="example@example.com"/>
  </plugin-java>
</plugin>