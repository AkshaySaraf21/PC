<?xml version="1.0"?>
<delegate
  xmlns="http://guidewire.com/datamodel"
  name="StateDrivenLookup"
  subpackage="availability">
  <typekey
    name="State"
    typelist="Jurisdiction"
    desc="The state for which this lookup applies, or null if the lookup is not restricted by state"
    nullok="true"/>
  <fulldescription>Delegate for availability lookup entities where State is an optional restriction.</fulldescription>
</delegate>
