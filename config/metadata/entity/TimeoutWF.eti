<?xml version="1.0"?>
<subtype
  xmlns="http://guidewire.com/datamodel"
  entity="TimeoutWF"
  subpackage="workflow"
  desc="Workflow for timeouts"
  supertype="PolicyPeriodWorkflow">
  <column
    name="FunctionToCall"
    type="shorttext"
    desc="The callback function name to call when the timeout is over"
    nullok="false"/>
  <column
    name="WakeupTime"
    type="datetime"
    desc="The date and time at which the workflow should wakeup"
    nullok="false"/>
</subtype>