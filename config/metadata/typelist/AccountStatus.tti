<?xml version="1.0"?>
<typelist xmlns="http://guidewire.com/typelists" desc="The status of the account" final="false" subpackage="account" name="AccountStatus">
  <typecode code="Pending" desc="The account is ready for data entry, but data entry is still ongoing and the account is not considered fully open" name="Pending"/>
  <typecode code="Active" desc="The account is fully ready for open and submissions have been created for it" name="Active"/>
  <typecode code="Withdrawn" desc="The account has been withdrawn from consideration for business with the carrier" name="Withdrawn"/>
  <typecode code="Merged" desc="The account has been merged into another account and is available for read only access." name="Merged"/>
</typelist>
