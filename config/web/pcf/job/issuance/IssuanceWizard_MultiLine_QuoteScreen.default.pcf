<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Screen
    id="IssuanceWizard_MultiLine_QuoteScreen"
    mode="default">
    <Require
      name="issuance"
      type="Issuance"/>
    <Require
      name="policyPeriod"
      type="PolicyPeriod"/>
    <Require
      name="isEditable"
      type="boolean"/>
    <Require
      name="jobWizardHelper"
      type="web.job.JobWizardHelper"/>
    <Toolbar>
      <WizardButtons/>
      <ToolbarButtonSetRef
        mode="policyPeriod.Job.Subtype"
        toolbarButtonSet="JobWizardToolbarButtonSet(policyPeriod, issuance, jobWizardHelper)"/>
      <ToolbarButton
        action="policyPeriod.printDocument(&quot;SubmissionQuote&quot;, &quot;Issuance Quote&quot;)"
        download="true"
        id="CreateIssuanceQuote"
        label="displaykey.Button.PrintQuote"
        shortcut="P"/>
    </Toolbar>
    <PanelRef
      def="Common_MultiLine_QuoteScreenPanelSet(policyPeriod, isEditable, jobWizardHelper, true)"
      mode="&quot;default&quot;"/>
  </Screen>
</PCF>