<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <EntryPoint
    desc="Opens the Job Wizard for the Job with the specified number"
    id="Job"
    location="JobNumberForward(JobNumber)">
    <EntryPointParameter
      locationParam="JobNumber"
      optional="true"
      type="String"/>
  </EntryPoint>
</PCF>