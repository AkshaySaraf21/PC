<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <InputSet
    id="EffectivePeriodInputSet">
    <Require
      name="criteria"
      type="gw.reinsurance.search.EffectivePeriodSearchCriteria"/>
    <TypeKeyInput
      editable="true"
      id="PeriodType"
      label="displaykey.Web.Reinsurance.Agreement.EffectiveDate"
      value="criteria.PeriodType">
      <PostOnChange/>
    </TypeKeyInput>
    <InputSet
      editable="criteria.PeriodType == TC_CUSTOM">
      <DateInput
        editable="true"
        id="EffectiveDate"
        label="displaykey.Web.Reinsurance.Agreement.From"
        value="criteria.From"/>
      <DateInput
        editable="true"
        id="ExpirationDate"
        label="displaykey.Web.Reinsurance.Agreement.To"
        value="criteria.To"/>
    </InputSet>
  </InputSet>
</PCF>