<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <DetailViewPanel
    id="HistoryScreenDV">
    <Require
      name="policyHistorySearch"
      type="gw.history.HistorySearchCriteria"/>
    <Require
      name="relatedtocriteria"
      type="Object[]"/>
    <InputColumn>
      <AltUserInput
        editable="true"
        id="user"
        label="displaykey.Web.PolicyFile.User"
        value="policyHistorySearch.User"/>
      <RangeInput
        editable="true"
        id="relatedto"
        label="displaykey.Web.History.RelatedTo"
        optionLabel="policyHistorySearch.getLevelDisplayString(VALUE)"
        sortValueRange="false"
        value="policyHistorySearch.RelatedItem"
        valueRange="relatedtocriteria"
        visible="relatedtocriteria != null and relatedtocriteria.length &gt; 1"/>
    </InputColumn>
    <InputColumn>
      <Label
        label="displaykey.Web.History.TimestampLabel"/>
      <DateInput
        editable="true"
        id="FromDate"
        label="displaykey.Web.History.FromDate"
        value="policyHistorySearch.FromDate"/>
      <DateInput
        editable="true"
        id="ToDate"
        label="displaykey.Web.History.ToDate"
        value="policyHistorySearch.ToDate"/>
    </InputColumn>
    <InputFooterSection>
      <InputSetRef
        def="SearchAndResetInputSet()"/>
    </InputFooterSection>
  </DetailViewPanel>
</PCF>