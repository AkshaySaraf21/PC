<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <ListViewPanel
    id="DocumentTemplateSearchResultsLV">
    <Require
      name="documentTemplateSearchResultsList"
      type="gw.plugin.document.IDocumentTemplateDescriptor[]"/>
    <RowIterator
      editable="false"
      elementName="documentTemplateSearchResult"
      pickValue="documentTemplateSearchResult"
      value="documentTemplateSearchResultsList">
      <Row>
        <Cell
          id="TemplateID"
          label="displaykey.Web.DocumentTemplateSearch.TemplateID"
          value="documentTemplateSearchResult.TemplateId"/>
        <Cell
          id="Name"
          label="displaykey.Web.DocumentTemplateSearch.Name"
          value="documentTemplateSearchResult.Name"/>
        <Cell
          id="DocumentType"
          label="displaykey.Web.DocumentTemplateSearch.DocumentType"
          sortBy="documentTemplateSearchResult.TemplateType"
          value="documentTemplateSearchResult.TemplateType as DocumentType"/>
        <Cell
          id="Language"
          label="displaykey.Web.DocumentTemplateSearch.Language"
          sortBy="documentTemplateSearchResult.Language"
          value="gw.api.util.LocaleUtil.toLanguageType(documentTemplateSearchResult.Language)"
          visible="LanguageType.getTypeKeys( false ).Count &gt; 1"/>
        <Cell
          id="DateModified"
          label="displaykey.Web.DocumentTemplateSearch.DateModified"
          value="documentTemplateSearchResult.DateModified"/>
        <Cell
          id="EffectiveDate"
          label="displaykey.Web.DocumentTemplateSearch.EffectiveDate"
          value="documentTemplateSearchResult.DateEffective"/>
        <Cell
          id="ExpirationDate"
          label="displaykey.Web.DocumentTemplateSearch.ExpirationDate"
          value="documentTemplateSearchResult.DateExpiration"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>