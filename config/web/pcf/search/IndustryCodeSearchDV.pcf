<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <DetailViewPanel
    id="IndustryCodeSearchDV">
    <Require
      name="searchCriteria"
      type="gw.product.IndustryCodeSearchCriteria"/>
    <InputColumn>
      <Input
        editable="true"
        id="Code"
        label="displaykey.Web.IndustryCodeSearch.Code"
        required="false"
        value="searchCriteria.Code"/>
      <Input
        editable="true"
        id="Classification"
        label="displaykey.Web.IndustryCodeSearch.Classification"
        required="false"
        value="searchCriteria.Classification"/>
    </InputColumn>
    <!-- <InputColumn>
      <Label
        label="displaykey.Web.IndustryCodeSearch.Date"/>
      <Input
        editable="true"
        id="Date"
        required="false"
        value="SearchCriteria.EffectiveOnDate"/>
    </InputColumn> -->
    <InputFooterSection>
      <InputSetRef
        def="SearchAndResetInputSet()"/>
    </InputFooterSection>
  </DetailViewPanel>
</PCF>