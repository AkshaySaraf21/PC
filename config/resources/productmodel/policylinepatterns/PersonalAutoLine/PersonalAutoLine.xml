<?xml version="1.0"?>
<PolicyLinePattern
  policyLineSubtype="PersonalAutoLine"
  priority="1"
  public-id="PersonalAutoLine"
  territoryCodeNeeded="true">
  <CoverageCategories>
    <CoverageCategory
      priority="60"
      public-id="PAMiscGrp"/>
    <CoverageCategory
      priority="3"
      public-id="PAPLiabGrp"/>
    <CoverageCategory
      priority="50"
      public-id="PAPPhysDamGrp"/>
    <CoverageCategory
      priority="110"
      public-id="PAPip"/>
    <CoverageCategory
      priority="49"
      public-id="PAPhysDamExcl"/>
    <CoverageCategory
      priority="48"
      public-id="PAPhysDamCond"/>
    <CoverageCategory
      priority="2"
      public-id="PALiabExcl"/>
    <CoverageCategory
      priority="1"
      public-id="PALiabCond"/>
    <CoverageCategory
      priority="440"
      public-id="PAPIPCat"/>
  </CoverageCategories>
  <CoverageSymbolGroupPatterns/>
  <InitializeScript/>
  <ModifierPatterns>
    <ModifierPattern
      displayEligibility="false"
      displayJustification="false"
      displayRange="false"
      displayValueFinal="false"
      lookupTableName="PAModifier"
      modifierDataType="typekey"
      modifierSubtype="PAModifier"
      priority="1000"
      public-id="PAMultiPolicyDiscount"
      referenceDateByType="PolicyTerm"
      scheduleRate="false"
      splitOnAnniversary="false"
      typeList="PAMultiPolicyDiscount">
      <RateFactorPatterns/>
    </ModifierPattern>
    <ModifierPattern
      displayEligibility="false"
      displayJustification="false"
      displayRange="false"
      displayValueFinal="false"
      lookupTableName="PAModifier"
      modifierDataType="boolean"
      modifierSubtype="PAModifier"
      priority="1000"
      public-id="PANoLossDiscount"
      referenceDateByType="PolicyTerm"
      scheduleRate="false"
      splitOnAnniversary="false">
      <RateFactorPatterns/>
    </ModifierPattern>
    <ModifierPattern
      lookupTableName="PAVehicleModifier"
      modifierDataType="boolean"
      modifierSubtype="PAVehicleModifier"
      priority="10"
      public-id="PAAntiLockBrakes"
      refCode="Anti_Lock Brakes Discount"
      referenceDateByType="PolicyTerm"/>
    <ModifierPattern
      lookupTableName="PAVehicleModifier"
      modifierDataType="typekey"
      modifierSubtype="PAVehicleModifier"
      priority="40"
      public-id="PAAntiTheft"
      referenceDateByType="PolicyTerm"
      typeList="AntiTheftType"/>
    <ModifierPattern
      lookupTableName="PAVehicleModifier"
      modifierDataType="typekey"
      modifierSubtype="PAVehicleModifier"
      priority="20"
      public-id="PAPassiveRestraint"
      referenceDateByType="PolicyTerm"
      typeList="PassiveRestraintType"/>
    <ModifierPattern
      lookupTableName="PAVehicleModifier"
      modifierDataType="rate"
      modifierSubtype="PAVehicleModifier"
      priority="-1"
      public-id="PASafetyBonusDiscount"
      referenceDateByType="PolicyTerm"
      scheduleRate="false">
      <AvailabilityScript><![CDATA[return (PersonalVehicle.getModifier("PAAntiLockBrakes").BooleanModifier)
   and (PersonalVehicle.getModifier("PAPassiveRestraint").TypeKeyModifier == "both")
   and (PersonalVehicle.Drivers.countWhere(\ v -> v.PolicyDriver.ApplicableGoodDriverDiscount) == PersonalVehicle.Drivers.Count)]]></AvailabilityScript>
    </ModifierPattern>
    <ModifierPattern
      lookupTableName="PAModifier"
      modifierDataType="rate"
      modifierSubtype="PAModifier"
      priority="-1"
      public-id="PAMultiCarDiscount"
      referenceDateByType="PolicyTerm"
      scheduleRate="false">
      <AvailabilityScript><![CDATA[return PersonalAutoLine.Vehicles.Count > 1]]></AvailabilityScript>
    </ModifierPattern>
  </ModifierPatterns>
  <OfficialIdPatterns/>
  <AvailableCoverageCurrencies>
    <AvailableCoverageCurrency
      currency="usd"
      public-id="PersonalAutoLineusd"/>
  </AvailableCoverageCurrencies>
</PolicyLinePattern>