<?xml version="1.0"?>
<Entity
  name="ContractorsEquipment">
  <Columns>
    <Column
      beanPath="ContractorsEquipment.ContractorsEquipmentNumber"
      name="number"/>
    <Column
      beanPath="ContractorsEquipment.Description"
      name="description"/>
  </Columns>
  <DisplayName><![CDATA[
return number + ": " + (description == null ? displaykey.EntityName.ContractorsEquipment.NoDescription : description)
    
  ]]></DisplayName>
</Entity>