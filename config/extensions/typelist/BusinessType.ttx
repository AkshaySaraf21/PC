<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Types of external organizations"
  name="BusinessType">
  <typecode
    code="insurer"
    desc="This is the one true carrier, one and only one of these should be defined, and it should be defined in the bootstrap."
    name="Insurer">
    <category
      code="carrier"
      typelist="BusinessTypeCategory"/>
    <category
      code="producer"
      typelist="BusinessTypeCategory"/>
  </typecode>
  <typecode
    code="agency"
    desc="This is an agency"
    name="Agency">
    <category
      code="producer"
      typelist="BusinessTypeCategory"/>
  </typecode>
  <typecode
    code="broker"
    desc="This is a broker"
    name="Broker">
    <category
      code="producer"
      typelist="BusinessTypeCategory"/>
  </typecode>
  <typecode
    code="mga"
    desc="Managing general agent"
    name="Managing general agent">
    <category
      code="producer"
      typelist="BusinessTypeCategory"/>
  </typecode>
  <typecode
    code="feeaudit"
    desc="Fee audit company"
    name="Fee audit company"/>
  <typecode
    code="feeinspect"
    desc="Fee inspection company"
    name="Fee inspection company"/>
</typelistextension>