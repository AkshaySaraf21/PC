<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Type of the document for access-restriction purposes"
  name="DocumentSecurityType">
  <typecode
    code="unrestricted"
    desc="Document that does not require access restriction"
    name="Unrestricted"/>
  <typecode
    code="internalonly"
    desc="Document that should not be viewed by people outside the company"
    name="Internal only"/>
  <typecode
    code="sensitive"
    desc="Document that is sensitive in nature"
    name="Sensitive"/>
</typelistextension>