<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Status of external producer organization"
  name="ProducerStatus">
  <typecode
    code="Active"
    desc="Normal privileges"
    name="Active"
    priority="10">
    <category
      code="submissionOkay"
      typelist="ProducerStatusUse"/>
    <category
      code="renewalOkay"
      typelist="ProducerStatusUse"/>
    <category
      code="okay"
      typelist="ProducerStatusUse"/>
  </typecode>
  <typecode
    code="Limited"
    desc="Reduced permissions, can still renew."
    name="Limited"
    priority="200">
    <category
      code="renewalOkay"
      typelist="ProducerStatusUse"/>
  </typecode>
  <typecode
    code="Suspended"
    desc="Reduced permissions, can no longer renew."
    name="Suspended"
    priority="300"/>
  <typecode
    code="Terminating"
    desc="The producer is going away."
    name="Terminating"
    priority="800"/>
  <typecode
    code="Terminated"
    desc="The producer relationship is stopped and the producer is no longer granted access to the system."
    name="Terminated"
    priority="1000"/>
</typelistextension>