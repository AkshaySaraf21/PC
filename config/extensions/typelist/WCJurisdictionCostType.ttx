<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="The type of a WC Jurisdiction Cost."
  name="WCJurisdictionCostType">
  <typecode
    code="MinPrem"
    desc="Additional premium charged so that standard premium for the policy or state meets a minimum amount"
    name="Minimum Premium Adjustment"/>
  <typecode
    code="CancelShortRatePenalty"
    desc="Cancellation penalty determined by short-rate."
    name="Cancellation short-rate penalty"/>
  <typecode
    code="Tax"
    desc="Tax"
    name="Tax"/>
  <typecode
    code="CIGA"
    desc="CA Insurance Guarantee Assoc surcharge"
    name="CIGA surcharge"/>
  <typecode
    code="ExpenseConst"
    desc="Expense constant"
    name="Expense constant"/>
  <typecode
    code="PremDis"
    desc="Premium discount"
    name="Premium discount"/>
  <typecode
    code="TerrorPrem"
    desc="Terrorism premium"
    name="Terrorism premium"/>
  <typecode
    code="SchedCredit"
    desc="Premium adjustment based on evaluation of the insured's risk relative to the average risk of others in the same class"
    name="Schedule credit"/>
  <typecode
    code="Waiver"
    desc="Extra charge for waiver of subrogation"
    name="Waiver charge"/>
  <typecode
    code="ExpMod"
    desc="Workers' comp experience modifier credit or debit"
    name="Experience modifier"/>
  <typecode
    code="WCEL"
    desc="Premium for workers' comp employers liability limits above the base (standard) amounts"
    name="Emp liab increased limits"/>
