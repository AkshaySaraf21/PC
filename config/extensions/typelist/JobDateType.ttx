<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="The date type (effective, written, reference) used to determine whether the job falls within the dates of a hold"
  name="JobDateType">
  <typecode
    code="Effective"
    desc="Effective Date"
    name="Effective Date"
    priority="10">
    <category
      code="UWHold"
      typelist="UWIssueCheckingSet"/>
  </typecode>
  <typecode
    code="Written"
    desc="Written Date"
    name="Written Date"
    priority="20">
    <category
      code="UWHold"
      typelist="UWIssueCheckingSet"/>
  </typecode>
  <typecode
    code="Reference"
    desc="Reference Date"
    name="Reference Date"
    priority="30">
    <category
      code="RegulatoryHold"
      typelist="UWIssueCheckingSet"/>
  </typecode>
</typelistextension>