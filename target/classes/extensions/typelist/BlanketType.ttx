<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Blanket type"
  name="BlanketType">
  <typecode
    code="decline"
    desc="Decline"
    name="Decline"
    priority="1"/>
  <typecode
    code="build"
    desc="Building"
    name="Building"
    priority="2"/>
  <typecode
    code="bus"
    desc="Business personal property"
    name="Business Personal Property"
    priority="3"/>
  <typecode
    code="busbuild"
    desc="Each building and business personal property"
    name="Building and Contents - by building"
    priority="4"/>
  <typecode
    code="location"
    desc="Per location: all building and business personal property"
    name="Building and Contents - by location"
    priority="5"/>
  <typecode
    code="all"
    desc="All building and business personal property"
    name="All Building and Contents"
    priority="6"/>
  <typefilter
    desc="Types of blanket for commercial property"
    name="CPBlanketType">
    <include
      code="multiloc"/>
    <include
      code="singleloc"/>
    <include
      code="singlecov"/>
  </typefilter>
  <typefilter
    desc="Types of blanket for business owners"
    name="BOPBlanketType">
    <include
      code="all"/>
    <include
      code="build"/>
    <include
      code="bus"/>
    <include
      code="busbuild"/>
    <include
      code="decline"/>
    <include
      code="location"/>
  </typefilter>
  <typecode
    code="singleloc"
    desc="One or more buildings at this location"
    name="Single Location"
    priority="7"/>
  <typecode
    code="multiloc"
    desc="One or more locations (including one or more buildings)"
    name="Multiple Locations"
    priority="8"/>
  <typecode
    code="singlecov"
    desc="Single coverage"
    name="Single Coverage"
    priority="9"/>
</typelistextension>