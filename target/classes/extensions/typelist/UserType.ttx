<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Types of normal system users"
  name="UserType">
  <typecode
    code="producer"
    desc="An external producer."
    name="Producer"
    priority="4">
    <category
      code="external"
      typelist="UserTypeCategory"/>
    <category
      code="internal"
      typelist="UserTypeCategory"/>
    <category
      code="producer"
      typelist="BusinessTypeCategory"/>
  </typecode>
  <typecode
    code="assistant"
    desc="Assistant for an external producer."
    name="Producer Assistant"
    priority="3">
    <category
      code="external"
      typelist="UserTypeCategory"/>
    <category
      code="internal"
      typelist="UserTypeCategory"/>
    <category
      code="producer"
      typelist="BusinessTypeCategory"/>
  </typecode>
  <typecode
    code="underwriter"
    desc="An underwriter."
    name="Underwriter"
    priority="2">
    <category
      code="internal"
      typelist="UserTypeCategory"/>
  </typecode>
  <typecode
    code="other"
    desc="Other."
    name="Other"
    priority="1">
    <category
      code="internal"
      typelist="UserTypeCategory"/>
    <category
      code="external"
      typelist="UserTypeCategory"/>
  </typecode>
  <typecode
    code="auditor"
    desc="An auditor"
    name="Auditor"
    priority="5">
    <category
      code="internal"
      typelist="UserTypeCategory"/>
    <category
      code="external"
      typelist="UserTypeCategory"/>
    <category
      code="feeaudit"
      typelist="BusinessType"/>
    <category
      code="insurer"
      typelist="BusinessType"/>
  </typecode>
  <typefilter
    desc="This type filter indicates which user types will view jobs by activity."
    name="ByActivity">
    <include
      code="assistant"/>
    <include
      code="underwriter"/>
    <include
      code="auditor"/>
    <include
      code="other"/>
  </typefilter>
  <typefilter
    desc="This type filter indicates which user types will view jobs by role."
    name="ByRole">
    <include
      code="producer"/>
  </typefilter>
</typelistextension>