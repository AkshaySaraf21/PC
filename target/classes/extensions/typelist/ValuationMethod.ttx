<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Types of valuation methods"
  name="ValuationMethod">
  <typecode
    code="Appraisal"
    desc="Appraisal"
    name="Appraisal"
    retired="true"/>
  <typecode
    code="CompSale"
    desc="Comparable sale"
    name="Comparable sale"
    retired="true"/>
  <typecode
    code="SaleRec"
    desc="Sales receipt"
    name="Sales receipt"
    retired="true"/>
  <typecode
    code="ReplCost"
    desc="Replacement cost"
    name="Replacement cost"
    priority="1">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
  </typecode>
  <typecode
    code="ACV"
    desc="Actual cash value"
    name="Actual cash value"
    priority="2">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
  </typecode>
  <typecode
    code="FuncValue"
    desc="Functional value"
    name="Functional value"
    priority="3">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
  </typecode>
  <typecode
    code="AgreedAmt"
    desc="Agreed amount"
    name="Agreed amount"
    priority="4">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
  </typecode>
  <typecode
    code="ActCost"
    desc="Actual cost"
    name="Actual cost"
    retired="true"/>
  <typefilter
    desc="Valuation Methods Buildings and Contents "
    name="BuildingContentsFilter">
    <include
      code="ReplCost"/>
    <include
      code="FuncValue"/>
    <include
      code="ACV"/>
  </typefilter>
  <typefilter
    desc="Valuation Methods for Dwellings"
    name="DwellingFilter">
    <include
      code="ReplCost"/>
    <include
      code="ACV"/>
  </typefilter>
  <typefilter
    desc="Valutation Methods for Contractors Equipment"
    name="ContractorsEquipmentFilter">
    <include
      code="ACV"/>
    <include
      code="ReplCost"/>
  </typefilter>
</typelistextension>