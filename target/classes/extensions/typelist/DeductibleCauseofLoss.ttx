<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Multiple deductible cause of loss"
  name="DeductibleCauseofLoss">
  <typecode
    code="1"
    desc="All covered causes of loss"
    name="1 All"/>
  <typecode
    code="2"
    desc="All Covered Causes of Loss except Windstorm or Hail"
    name="2 Except wind/hail"/>
  <typecode
    code="3"
    desc="All covered causes of loss except theft"
    name="3 Except theft"/>
  <typecode
    code="4"
    desc="All covered causes of loss except windstorm or hail and theft"
    name="4 Except wind/hail/theft"/>
  <typecode
    code="5"
    desc="Windstorm or hail"
    name="5 Windstorm and hail"/>
  <typecode
    code="6"
    desc="Theft"
    name="6 Theft"/>
</typelistextension>