<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Subtype typelist for entity Job"
  name="Job">
  <typefilter
    desc="Jobs that can be associated with a form pattern"
    name="FormPatternJobs">
    <include
      code="Cancellation"/>
    <include
      code="PolicyChange"/>
    <include
      code="Reinstatement"/>
    <include
      code="Renewal"/>
    <include
      code="Rewrite"/>
    <include
      code="RewriteNewAccount"/>
    <include
      code="Submission"/>
  </typefilter>
  <typefilter
    desc="Jobs that can be associated with a policy hold "
    name="PolicyHoldJobs">
    <include
      code="Issuance"/>
    <include
      code="PolicyChange"/>
    <include
      code="Reinstatement"/>
    <include
      code="Renewal"/>
    <include
      code="Rewrite"/>
    <include
      code="Submission"/>
    <include
      code="RewriteNewAccount"/>
  </typefilter>
  <typefilter
    desc="Specifies the Job types that show up under Other Work Orders on the Desktop and Team Screen"
    name="OtherWorkOrders">
    <include
      code="PolicyChange"/>
    <include
      code="Cancellation"/>
    <include
      code="Reinstatement"/>
    <include
      code="Rewrite"/>
    <include
      code="Audit"/>
    <include
      code="RewriteNewAccount"/>
  </typefilter>
</typelistextension>