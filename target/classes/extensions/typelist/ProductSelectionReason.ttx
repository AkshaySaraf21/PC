<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Reason for making a product offer (filtered by ProductSelectionStatus)"
  name="ProductSelectionReason">
  <typecode
    code="NotOffered"
    desc="Product not offered"
    name="NotOffered">
    <category
      code="Unavailable"
      typelist="ProductSelectionStatus"/>
    <category
      code="AutoDeclined"
      typelist="ProductSelectionStatus"/>
  </typecode>
  <typecode
    code="LossHistory"
    desc="Loss history"
    name="Loss history">
    <category
      code="AutoDeclined"
      typelist="ProductSelectionStatus"/>
  </typecode>
  <typecode
    code="PaymentHistory"
    desc="Payment history"
    name="Payment history">
    <category
      code="AutoDeclined"
      typelist="ProductSelectionStatus"/>
  </typecode>
</typelistextension>