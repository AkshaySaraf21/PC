<?xml version="1.0"?>
<typelistextension
        xmlns="http://guidewire.com/typelists"
        desc="Rounding scale applied to calculations in the Rate Routine."
        name="RoundingScaleType">
    <typecode code="4" name=".0001" priority="1" desc="ten thousandths"/>
    <typecode code="3" name=".001" priority="1" desc="thousandths"/>
    <typecode code="2" name=".01" priority="2" desc="hundredths"/>
    <typecode code="1" name=".1" priority="3" desc="tenths"/>
    <typecode code="0" name="1" priority="4" desc="ones"/>
    <typecode code="minus1" name="10" priority="5" desc="tens"/>
    <typecode code="minus2" name="100" priority="6" desc="hundreds"/>
    <typecode code="minus3" name="1000" priority="7" desc="thousands"/>
</typelistextension>
