  <xs:complexType name="CCPersonVendor">
    <xs:complexContent>
      <xs:extension base="tns:CCPerson">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CCAdjudicator">
