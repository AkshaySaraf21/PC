<?xml version="1.0"?>
<Entity
  name="PolicyTerm">
  <Columns>
    <Column
      beanPath="PolicyTerm.PolicyNumber"
      name="policyNumber"/>
    <Column
      beanPath="PolicyTerm.Periods"
      name="periods"/>
  </Columns>
  <DisplayName><![CDATA[var sb = new java.lang.StringBuffer()
sb.append(policyNumber)
if (periods[0] != null) {
  sb.append("-").append(periods[0].TermNumber)
}
return sb.toString()

]]></DisplayName>
</Entity>