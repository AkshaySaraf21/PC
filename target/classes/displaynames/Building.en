<?xml version="1.0"?>
<Entity
  name="Building">
  <Columns>
    <Column
      beanPath="Building.BuildingNum"
      name="number"/>
    <Column
      beanPath="Building.Description"
      name="description"/>
  </Columns>
  <DisplayName><![CDATA[if (description.HasContent) {
 return displaykey.EntityName.Building.byDescription(number, description)
} 
return displaykey.EntityName.Building.byNumber(number)]]></DisplayName>
</Entity>