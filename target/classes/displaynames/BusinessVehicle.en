<?xml version="1.0"?>
<Entity
  name="BusinessVehicle">
  <Columns>
    <Column
      beanPath="BusinessVehicle.Year"
      name="year"
      sortOrder="1"/>
    <Column
      beanPath="BusinessVehicle.Make"
      name="make"
      sortOrder="2"/>
    <Column
      beanPath="BusinessVehicle.Model"
      name="model"
      sortOrder="3"/>
    <Column
      beanPath="BusinessVehicle.Location"
      name="state"
      sortOrder="4"/>
    <Column
      beanPath="BusinessVehicle.LicensePlate"
      name="licensePlate"
      sortOrder="5"/>
    <Column
      beanPath="BusinessVehicle.LicenseState"
      name="licenseState"
      sortOrder="6"/>
  </Columns>
  <DisplayName><![CDATA[
var sb = new java.lang.StringBuffer()
if (year != null) {
  sb.append(year as int).append(" ")
}
if (make.HasContent) {
  sb.append(make).append(" ")
}
if (model.HasContent) {
  sb.append(model).append(" ")
}
if (state != null) {
  sb.append(displaykey.EntityName.BusinessVehicle.InState(state.State.DisplayName)).append(" ")
}
if (licensePlate.HasContent) {
  sb.append("(").append(licensePlate)
  if (licenseState != null) {
    sb.append("/").append(licenseState.Code)
  }
  sb.append(")")
}

if (sb.length() == 0) {
  sb.append(displaykey.EntityName.BusinessVehicle.NewlyCreated)
}

return sb.toString()
    
  ]]></DisplayName>
</Entity>