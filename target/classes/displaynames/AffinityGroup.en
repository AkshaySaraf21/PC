<?xml version="1.0"?>
<Entity
  name="AffinityGroup">
  <Columns>
    <Column
      beanPath="AffinityGroup.Name"
      name="Name"
      sortOrder="0"/>
    <Column
      beanPath="AffinityGroup.NameKanji"
      name="NameKanji"/>
  </Columns>
  <DisplayName><![CDATA[uses gw.api.util.LocaleUtil

return (LocaleUtil.CurrentLanguageType.Code == "ja_JP" and NameKanji.HasContent) ? NameKanji : Name]]></DisplayName>
</Entity>