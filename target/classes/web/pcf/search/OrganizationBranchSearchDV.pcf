<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <DetailViewPanel
    id="OrganizationBranchSearchDV">
    <Require
      name="searchCriteria"
      type="GroupSearchCriteria"/>
    <InputColumn>
      <Input
        editable="true"
        id="Name"
        label="displaykey.Web.OrganizationBranchSearch.BranchName"
        value="searchCriteria.BranchName"/>
      <Input
        editable="true"
        id="BranchCode"
        label="displaykey.Web.OrganizationBranchSearch.BranchCode"
        value="searchCriteria.BranchCode"/>
    </InputColumn>
    <InputFooterSection>
      <InputSetRef
        def="SearchAndResetInputSet()"/>
    </InputFooterSection>
  </DetailViewPanel>
</PCF>