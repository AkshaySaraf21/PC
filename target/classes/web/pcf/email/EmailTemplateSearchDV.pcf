<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <DetailViewPanel
    id="EmailTemplateSearchDV">
    <Require
      name="emailTemplateSearchCriteria"
      type="gw.api.email.EmailTemplateSearchCriteria"/>
    <InputColumn>
      <Input
        editable="true"
        id="Topic"
        label="displaykey.Web.EmailTemplateSearch.Topic"
        value="emailTemplateSearchCriteria.Topic"/>
      <TypeKeyInput
        editable="true"
        id="Language"
        label="displaykey.Web.EmailTemplateSearch.Language"
        value="emailTemplateSearchCriteria.Language"
        visible="LanguageType.getTypeKeys( false ).Count &gt; 1"/>
      <Input
        editable="true"
        id="Keywords"
        label="displaykey.Web.EmailTemplateSearch.Keywords"
        value="emailTemplateSearchCriteria.Keywords"/>
    </InputColumn>
    <InputFooterSection>
      <InputSetRef
        def="SearchAndResetInputSet()"/>
    </InputFooterSection>
  </DetailViewPanel>
</PCF>