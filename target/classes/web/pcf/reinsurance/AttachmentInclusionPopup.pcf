<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <Popup
    beforeCommit="agreement.updateInclusion(risk, inclusion)"
    canEdit="true"
    id="AttachmentInclusionPopup"
    startInEditMode="true"
    title="displaykey.Web.Reinsurance.Attachment.InclusionPopup.Title">
    <LocationEntryPoint
      signature="AttachmentInclusionPopup(agreement : RIAgreement, risk : RIRisk)"/>
    <Variable
      name="agreement"
      type="RIAgreement"/>
    <Variable
      name="risk"
      type="RIRisk"/>
    <Variable
      initialValue="agreement.getInclusion(risk)"
      name="inclusion"
      type="typekey.RIAttachmentInclusionType"/>
    <Screen>
      <Toolbar>
        <EditButtons/>
      </Toolbar>
      <DetailViewPanel>
        <InputColumn>
          <TypeKeyInput
            editable="true"
            id="Inclusion"
            label="displaykey.Web.Reinsurance.Attachment.Inclusion"
            required="true"
            value="inclusion"/>
        </InputColumn>
      </DetailViewPanel>
    </Screen>
  </Popup>
</PCF>