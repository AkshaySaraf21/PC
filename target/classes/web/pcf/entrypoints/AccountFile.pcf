<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <EntryPoint
    desc="Opens the Account File for the Account with the specified account number"
    id="AccountFile"
    location="AccountFileDoRetrievalForward(AccountNumber)">
    <EntryPointParameter
      locationParam="AccountNumber"
      optional="true"
      type="String"/>
  </EntryPoint>
</PCF>