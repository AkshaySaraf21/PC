<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <ListViewPanel
    id="AffinityGroupJurisdictionsLV">
    <ExposeIterator
      valueType="AffinityGroupJurisdiction"
      widget="AffinityGroupJurisdictionsLV"/>
    <Require
      name="affinityGroup"
      type="AffinityGroup"/>
    <RowIterator
      checkBoxVisible="true"
      editable="true"
      elementName="AffinityGroupJurisdiction"
      hasCheckBoxes="true"
      hideCheckBoxesIfReadOnly="true"
      toAdd="affinityGroup.addToJurisdictions(AffinityGroupJurisdiction)"
      toRemove="affinityGroup.removeFromJurisdictions(AffinityGroupJurisdiction)"
      value="affinityGroup.Jurisdictions">
      <IteratorSort
        sortBy="AffinityGroupJurisdiction.Jurisdiction"
        sortOrder="1"/>
      <Row>
        <TypeKeyCell
          align="left"
          editable="true"
          id="Jurisdiction"
          label="displaykey.Web.Admin.AffinityGroupDetail.Jurisdictions.Jurisdiction"
          required="true"
          value="AffinityGroupJurisdiction.Jurisdiction">
          <PostOnChange
            onChange="gw.admin.affinitygroup.AffinityGroupUIHelper.checkDuplicateJurisdictions(AffinityGroup)"/>
        </TypeKeyCell>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>