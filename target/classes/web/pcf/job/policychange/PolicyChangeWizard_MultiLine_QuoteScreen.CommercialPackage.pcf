<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Screen
    editable="false"
    id="PolicyChangeWizard_MultiLine_QuoteScreen"
    mode="CommercialPackage">
    <Require
      name="job"
      type="Job"/>
    <Require
      name="policyPeriod"
      type="PolicyPeriod"/>
    <Require
      name="isEditable"
      type="boolean"/>
    <Require
      name="jobWizardHelper"
      type="web.job.JobWizardHelper"/>
    <Toolbar>
      <WizardButtons/>
      <ToolbarButtonSetRef
        mode="policyPeriod.Job.Subtype"
        toolbarButtonSet="JobWizardToolbarButtonSet(policyPeriod, job, jobWizardHelper)"/>
      <ToolbarButton
        action="policyPeriod.printDocument(&quot;PolicyChangeQuote&quot;, &quot;Policy Change Quote&quot;)"
        download="true"
        id="CreatePolicyChangeQuote"
        label="displaykey.Button.PrintQuote"
        shortcut="P"/>
    </Toolbar>
    <PanelRef
      def="Common_MultiLine_QuoteScreenPanelSet(policyPeriod, isEditable, jobWizardHelper, true)"
      mode="&quot;CommercialPackage&quot;"/>
  </Screen>
</PCF>