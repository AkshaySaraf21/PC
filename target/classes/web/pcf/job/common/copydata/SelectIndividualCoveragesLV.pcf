<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <ListViewPanel
    editable="true"
    id="SelectIndividualCoveragesLV">
    <Require
      name="coverageCopiers"
      type="List&lt;gw.coverage.ClausePatternCopier&gt;"/>
    <RowIterator
      canPick="false"
      editable="true"
      elementName="coverageCopier"
      id="coverageIterator"
      pageSize="0"
      value="coverageCopiers">
      <Row>
        <CheckBoxCell
          align="left"
          editable="true"
          id="CoverageCopierShouldCopy"
          value="coverageCopier.ShouldCopy"/>
        <TextCell
          align="left"
          enableSort="false"
          id="CoverageDescription"
          label="displaykey.Web.CopyPolicyData.PersonalAuto.Coverages.Coverage.Title"
          value="coverageCopier.Source"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>