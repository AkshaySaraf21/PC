<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <RowSet
    id="GLAuditStateCostRowSet"
    mode="total">
    <Require
      name="auditCostWrapper"
      type="ui.AuditCostWrapper"/>
    <Row
      visible="auditCostWrapper.Bold">
      <Cell
        id="Empty1"/>
      <Cell
        id="Empty2"/>
      <Cell
        bold="true"
        id="Description"
        value="auditCostWrapper.Description"/>
      <Cell
        id="Empty3"/>
      <Cell
        id="Empty4"/>
      <Cell
        id="Empty5"/>
      <MonetaryAmountCell
        align="right"
        bold="true"
        formatType="currency"
        id="EstimatedPremium"
        value="auditCostWrapper.EstimatedTotal"/>
      <MonetaryAmountCell
        align="right"
        bold="true"
        formatType="currency"
        id="AuditedPremium"
        value="auditCostWrapper.AuditedTotal"/>
      <MonetaryAmountCell
        align="right"
        bold="true"
        formatType="currency"
        id="Change"
        value="auditCostWrapper.Change"/>
    </Row>
    <Row
      visible="not auditCostWrapper.Bold">
      <Cell
        id="Empty8"/>
      <Cell
        id="Empty9"/>
      <Cell
        id="SubDescription"
        value="auditCostWrapper.Description"/>
      <Cell
        id="Empty10"/>
      <Cell
        id="Empty11"/>
      <Cell
        id="Empty12"/>
      <MonetaryAmountCell
        align="right"
        formatType="currency"
        id="SubEstimatedPremium"
        value="auditCostWrapper.EstimatedTotal"/>
      <MonetaryAmountCell
        align="right"
        formatType="currency"
        id="SubAuditedPremium"
        value="auditCostWrapper.AuditedTotal"/>
      <MonetaryAmountCell
        align="right"
        formatType="currency"
        id="SubChange"
        value="auditCostWrapper.Change"/>
    </Row>
  </RowSet>
</PCF>