<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <RowSet
    id="StateCostRowSet"
    mode="total">
    <Require
      name="costWrapper"
      type="ui.CostWrapper"/>
    <Row
      visible="costWrapper.Bold">
      <Cell
        id="Empty1"/>
      <Cell
        id="Empty2"/>
      <Cell
        bold="true"
        id="description"
        value="costWrapper.Description"
        wrap="true"/>
      <Cell
        id="Empty3"/>
      <Cell
        id="Empty4"/>
      <MonetaryAmountCell
        align="right"
        bold="true"
        formatType="currency"
        id="total"
        value="costWrapper.Total"/>
    </Row>
    <Row
      visible="not costWrapper.Bold">
      <Cell
        id="Empty5"/>
      <Cell
        id="Empty6"/>
      <Cell
        id="subDescription"
        value="costWrapper.Description"
        wrap="true"/>
      <Cell
        id="Empty7"/>
      <Cell
        id="Empty8"/>
      <MonetaryAmountCell
        align="right"
        formatType="currency"
        id="subTotal"
        value="costWrapper.Total"/>
    </Row>
  </RowSet>
</PCF>