<?xml version="1.0"?>
<typelist xmlns="http://guidewire.com/typelists" desc="Defines if a PolicyCenter entity's existence on a policy is required, suggested, or electable." final="true" subpackage="policy" name="ExistenceType">
  <typecode code="Required" desc="Entity is required to exist on the policy if it is available." name="Required"/>
  <typecode code="Suggested" desc="Entity is initially added to the policy if it is available, but may be removed/declined by the user." name="Suggested"/>
  <typecode code="Electable" desc="Entity is initially NOT added to the policy, but may be added to the policy by the user if it is available." name="Electable"/>
</typelist>
