<xs:element name="wlm_conn_env" >
  <xs:annotation>
     <xs:documentation>
        ----------------------------------------------------------------------
        WLM Connection Environment

        This schema describes the structure of the workload
        manager connection environment that is obtained by
        calling the WLM_GET_CONN_ENV table function.
        ----------------------------------------------------------------------
     </xs:documentation>
  </xs:annotation>
  <xs:complexType>
     <xs:sequence>
        <xs:element name = "collectactdata"
                    type = "db2_string_type_255" >
          <xs:annotation>
            <xs:documentation>
            Connection COLLECT ACTIVITY DATA setting. Possible values include "WITHOUT DETAILS",
            "WITH DETAILS", "WITH DETAILS, SECTION", "WITH DETAILS, SECTION AND VALUES", and
            "WITH DETAILS AND VALUES".
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <xs:element name = "collectactpartition"
                    type = "db2_string_type_255" >
          <xs:annotation>
            <xs:documentation>
            Connection COLLECT ACTIVITY DATA partition setting.
            Possible values include "COORDINATOR" and "ALL".
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <xs:element name = "collectsectionactuals"
                    type = "db2_string_type_255" >
          <xs:annotation>
            <xs:documentation>
            Specifies whether to collect section actuals. Possible values include "NONE" and "BASE".
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip" />
     </xs:sequence>

     <xs:attribute name = "release"
                   type = "xs:long"
                   use  = "required" > 
        <xs:annotation>
          <xs:documentation>
          Represents the DB2 product level for which the WLM connection environment is being reported.
          </xs:documentation>
        </xs:annotation>
     </xs:attribute>
  
     <xs:anyAttribute processContents="skip" />
  </xs:complexType>
</xs:element>

